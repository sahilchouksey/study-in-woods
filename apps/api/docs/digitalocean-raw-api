"description": "example string",
"metrics": [
"example string"
],
"name": "example name",
"star_metric": {
"metric_uuid": "123e4567-e89b-12d3-a456-426614174000",
"name": "example name",
"success_threshold": 123,
"success_threshold_pct": 123
},
"workspace_uuid": "123e4567-e89b-12d3-a456-426614174000"
}
